<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <system:Boolean x:Key="IsDarkMode">true</system:Boolean>
  
  <!-- Background Colors -->
  <Color x:Key="BackgroundColor">#4B2C20</Color> <!-- Dark brownish -->
  <Color x:Key="BackgroundColorPointerOver">#6F3F2F</Color> <!-- Lighter brown -->
  <Color x:Key="BackgroundColorPressed">#8C5B44</Color> <!-- Warm orange brown -->
  <Color x:Key="BackgroundColorDisabled">#5A3B2B</Color> <!-- Muted brown -->

  <!-- Foreground Colors -->
  <Color x:Key="ForegroundColor">#F4E1B2</Color> <!-- Light beige -->
  <Color x:Key="ForegroundColorPointerOver">#FFFFFF</Color> <!-- White for hover -->
  <Color x:Key="ForegroundColorPressed">#FFD966</Color> <!-- Soft yellow -->
  <Color x:Key="ForegroundColorDisabled">#C2A87F</Color> <!-- Muted yellowish beige -->

  <!-- Border Colors -->
  <Color x:Key="BorderColor">#6F3F2F</Color> <!-- Dark brown -->
  <Color x:Key="BorderColorPointerOver">#8C5B44</Color> <!-- Light brownish-orange -->

  <!-- Accent Colors -->
  <Color x:Key="SystemAccentColor">#D97F3F</Color> <!-- Warm orange -->
  <Color x:Key="SystemAccentColorLight1">#F0A859</Color> <!-- Lighter orange -->
  <Color x:Key="SystemAccentColorDark1">#B3632D</Color> <!-- Darker orange brown -->

  <Color x:Key="NeutralAccentColor">#9C7F61</Color> <!-- Soft brownish beige -->
  <Color x:Key="NeutralAccentColorPointerOver">#C19F7A</Color> <!-- Lighter brown -->
  <Color x:Key="AccentColor1">#D97F3F</Color> <!-- Warm orange -->
  <Color x:Key="AccentColor2">#FF9F55</Color> <!-- Light orange -->
  <Color x:Key="AccentColor3">#E0623E</Color> <!-- Reddish orange -->

  <!-- Additional Colors -->
  <Color x:Key="TickLineColor">#8C5B44</Color> <!-- Warm brown -->
  <Color x:Key="BarNumberColor">#F4E1B2</Color> <!-- Light beige -->
  <Color x:Key="FinalPitchColor">#F4E1B2</Color> <!-- Light beige -->
  <Color x:Key="TrackBackgroundAltColor">#6F3F2F</Color> <!-- Lighter brown -->

  <!-- Keyboard Background -->
  <Color x:Key="WhiteKeyColorLeft">#D98F4F</Color> <!-- Warm orange brown -->
  <Color x:Key="WhiteKeyColorRight">#FFBA6B</Color> <!-- Soft orange -->
  <Color x:Key="WhiteKeyNameColor">#FFFFFF</Color> <!-- White text -->

  <Color x:Key="CenterKeyColorLeft">#C7A7A0</Color> <!-- Soft beige -->
  <Color x:Key="CenterKeyColorRight">#F1D5A5</Color> <!-- Light cream -->
  <Color x:Key="CenterKeyNameColor">#FF9F55</Color> <!-- Light orange -->

  <Color x:Key="BlackKeyColorLeft">Transparent</Color>
  <Color x:Key="BlackKeyColorRight">Transparent</Color>
  <Color x:Key="BlackKeyNameColor">#FFFFFF</Color> <!-- White text -->
</ResourceDictionary>
