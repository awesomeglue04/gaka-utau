<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <system:Boolean x:Key="IsDarkMode">false</system:Boolean>

  <!-- Background Colors -->
  <Color x:Key="BackgroundColor">#FFF4E1</Color> <!-- Light orange-ish background -->
  <Color x:Key="BackgroundColorPointerOver">#FFE5B3</Color> <!-- Lighter orange on hover -->
  <Color x:Key="BackgroundColorPressed">#FFD27F</Color> <!-- Soft orange when pressed -->
  <Color x:Key="BackgroundColorDisabled">#F6D19C</Color> <!-- Light muted orange when disabled -->

  <!-- Foreground Colors -->
  <Color x:Key="ForegroundColor">#5A3F29</Color> <!-- Darker brownish-orange for text -->
  <Color x:Key="ForegroundColorPointerOver">#D77F32</Color> <!-- Darker orange when hovered -->
  <Color x:Key="ForegroundColorPressed">#8C572D</Color> <!-- Darker orange-brown when pressed -->
  <Color x:Key="ForegroundColorDisabled">#B09C6A</Color> <!-- Muted light brown for disabled text -->

  <!-- Border Colors -->
  <Color x:Key="BorderColor">#FFB84D</Color> <!-- Light orange border -->
  <Color x:Key="BorderColorPointerOver">#FF9A33</Color> <!-- Darker orange border on hover -->

  <!-- Accent Colors -->
  <Color x:Key="SystemAccentColor">#FF914D</Color> <!-- Warm orange accent -->
  <Color x:Key="SystemAccentColorLight1">#FFB270</Color> <!-- Lighter orange accent -->
  <Color x:Key="SystemAccentColorDark1">#F67C23</Color> <!-- Darker orange accent -->

  <Color x:Key="NeutralAccentColor">#D6A97D</Color> <!-- Light brownish-orange for neutral accents -->
  <Color x:Key="NeutralAccentColorPointerOver">#E3B88F</Color> <!-- Lighter neutral accent on hover -->
  <Color x:Key="AccentColor1">#FF914D</Color> <!-- Orange accent -->
  <Color x:Key="AccentColor2">#FF8C61</Color> <!-- Soft coral orange -->
  <Color x:Key="AccentColor3">#FF6E5B</Color> <!-- Reddish orange accent -->

  <!-- Additional Colors -->
  <Color x:Key="TickLineColor">#D89C6C</Color> <!-- Light orange-brown for tick lines -->
  <Color x:Key="BarNumberColor">#D89C6C</Color> <!-- Light orange-brown for bar numbers -->
  <Color x:Key="FinalPitchColor">#E8C285</Color> <!-- Warm beige for final pitch -->
  <Color x:Key="TrackBackgroundAltColor">#FFE5B3</Color> <!-- Lighter orange background -->

  <!-- Keyboard Background -->
  <Color x:Key="WhiteKeyColorLeft">#F7C29E</Color> <!-- Soft light orange for left white keys -->
  <Color x:Key="WhiteKeyColorRight">#F1B67A</Color> <!-- Light warm orange for right white keys -->
  <Color x:Key="WhiteKeyNameColor">#FF6F30</Color> <!-- Vibrant orange for key names -->

  <Color x:Key="CenterKeyColorLeft">#FFB49E</Color> <!-- Light peach for left center keys -->
  <Color x:Key="CenterKeyColorRight">#FF9F70</Color> <!-- Soft orange for right center keys -->
  <Color x:Key="CenterKeyNameColor">#FF914D</Color> <!-- Bold orange for center key names -->

  <Color x:Key="BlackKeyColorLeft">#FF7D6F</Color> <!-- Reddish-orange for left black keys -->
  <Color x:Key="BlackKeyColorRight">#FF6F30</Color> <!-- Bright orange for right black keys -->
  <Color x:Key="BlackKeyNameColor">#FFFFFF</Color> <!-- White for black key names -->
</ResourceDictionary>
